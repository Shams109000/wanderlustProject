<% if(success && success.length){ %>
<div
  class="alert alert-success alert-dismissable fade show col-6 offset-3 d-flex justify-content-between"
  role="alert"
>
<div>
    <%=success %>

</div>
<div>
    <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
    
  ></button>
</div>
 
</div>

<% } %>
<% if(error && error.length){ %>
    <div
      class="alert alert-danger alert-dismissable fade show col-6 offset-3 d-flex justify-content-between"
      role="alert"
    >
    <div>
        <%=error %>
    
    </div>
    <div>
        <button
        type="button"
        class="btn-close"
        data-bs-dismiss="alert"
        aria-label="Close"
        
      ></button>
    </div>
     
    </div>
    
    <% } %>
    
